{
  "name": "tiny-typescript-logger",
  "author": "Christoffer Artmann",
  "version": "1.1.0",
  "license": "MIT",
  "main": "dist/index.js",
  "typings": "dist/index.d.ts",
  "type": "module",
  "files": [
    "dist",
    "src"
  ],
  "engines": {
    "node": ">=14"
  },
  "scripts": {
    "analyze": "size-limit --why",
    "build": "tsdx build",
    "format": "prettier --write .",
    "lint": "eslint ./src",
    "prepare": "tsdx build",
    "size": "size-limit",
    "start": "tsdx watch",
    "test": "vitest"
  },
  "peerDependencies": {
    "chalk": "^5.3.0"
  },
  "husky": {
    "hooks": {
      "pre-commit": "tsdx lint"
    }
  },
  "module": "dist/tiny-typescript-logger.esm.js",
  "size-limit": [
    {
      "path": "dist/tiny-typescript-logger.cjs.production.min.js",
      "limit": "10 KB"
    },
    {
      "path": "dist/tiny-typescript-logger.esm.js",
      "limit": "10 KB"
    }
  ],
  "devDependencies": {
    "@size-limit/preset-small-lib": "^11.1.4",
    "@typescript-eslint/eslint-plugin": "^8.3.0",
    "@typescript-eslint/parser": "^8.3.0",
    "chalk": "^5.3.0",
    "cross-env": "^7.0.3",
    "eslint": "^9.9.1",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.2.1",
    "husky": "^9.1.5",
    "prettier": "^3.3.3",
    "size-limit": "^11.1.4",
    "tsdx": "^0.14.1",
    "tslib": "^2.7.0",
    "typescript": "^5.5.4",
    "vitest": "^2.0.5"
  }
}
